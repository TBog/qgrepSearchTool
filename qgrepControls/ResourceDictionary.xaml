                            <Setter Property="BorderBrush" Value="{DynamicResource CheckBox.IsMouseOver.Border}" />
                            <Setter Property="Fill" TargetName="CheckMark" Value="{DynamicResource CheckBox.IsMouseOver.Mark}" />
